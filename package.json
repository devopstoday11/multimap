  },
  "prettier": "@github/prettier-config",
  "devDependencies": {
    "@github/prettier-config": "0.0.4",
    "chai": "^4.1.2",
    "eslint": "^7.0.0",
    "eslint-plugin-github": "^3.4.1",
    "mocha": "^7.1.2",
    "typescript": "^3.9.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:github/browser",
      "plugin:github/es6",
      "plugin:github/typescript"
    ]
  }
}
