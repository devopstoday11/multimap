{
  "name": "@github/multimap",
  "version": "0.0.1",
  "description": "Store multiple values under each map key.",
  "main": "dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": "github/multimap",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint src/*.ts test/*.js",
